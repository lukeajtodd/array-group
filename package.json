{
  "name": "general-utils-lt",
  "version": "0.0.1",
  "description": "Group elements within an array using a positive integer to determine the point to break the array down.",
  "main": "./dist/index-cjs.js",
  "module": "./dist/index-esm.js",
  "exports": {
    ".": {
      "require": "./dist/index-cjs.js",
      "import": "./dist/index-esm.js",
      "browser": {
        "module": "./dist/index-esm.js",
        "script": "./dist/index-iife.js"
      },
      "node": "./dist/index-cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "redrun types build-iife-browser build-esm-browser build-node -p",
    "types": "tsc index.ts -noEmit",
    "build-esm-browser": "esbuild index.ts --bundle --minify --sourcemap --target=chrome58,firefox57,safari11,edge16 --format=esm --outfile=dist/index-esm.js",
    "build-iife-browser": "esbuild index.ts --bundle --minify --sourcemap --target=chrome58,firefox57,safari11,edge16 --outfile=dist/index-iife.js",
    "build-node": "esbuild index.ts --bundle --platform=node --target=node10 --format=cjs --outfile=dist/index-cjs.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "array",
    "esbuild",
    "grouping",
    "unflatten"
  ],
  "author": "Luke Todd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeajtodd/general-utils-lt.git"
  },
  "devDependencies": {
    "esbuild": "^0.8.11",
    "redrun": "^8.0.3",
    "typescript": "^4.1.2"
  }
}
